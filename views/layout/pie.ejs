<% if (typeof alertMessage !== 'undefined') { %>
    <script>
        Swal.fire({
            title: 'Error',
            text: '<%= alertMessage %>',
            icon: '<%= icon%>',
            confirmButtonText: 'OK'
        }).then(()=>{
          window.history.back();
        })
    </script>
<% } %>
